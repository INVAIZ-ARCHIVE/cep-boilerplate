{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "extendscript/extendscript": true
  },
  "plugins": ["jsdoc", "extendscript"],
  "extends": ["eslint:recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-undef": "warn",
    "no-const-assign": "error"
  },
  "globals": {
    "$": true
  }
}
